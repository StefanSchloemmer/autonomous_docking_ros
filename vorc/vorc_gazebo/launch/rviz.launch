<?xml version="1.0"?>
<!-- Example of launch rviz with a custom rviz config to have selected topics preloaded -->
<launch>
  <arg name="rviz_config" default="$(find vorc_gazebo)/config/rviz_cora.rviz" />
  <node pkg="rviz" type="rviz"
        name="cora_visualization" args="-d $(arg rviz_config)" />
</launch>
